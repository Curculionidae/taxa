    "leaflet.markercluster": "^1.5.3",
    "markdown-it-anchor": "^8.6.7",
    "pinia": "^2.1.7",
    "unhead": "^1.8.10",
    "unplugin-vue-markdown": "^0.26.0",
    "vue": "^3.4.19",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
